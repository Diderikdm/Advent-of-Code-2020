with open("C:\\Advent\\day1.txt", 'r') as file:
    data = [int(x) for x in file.read().splitlines()]

for x in data:
    for y in data:
        if x+y == 2020:
            print('Part 1: {}'.format(x*y))
        for z in data:
            if x+y+z == 2020:
                print('Part 2: {}'.format(x*y*z))
